# GitHub Repository (for Docker image)
GITHUB_REPOSITORY=saturnbtc/titan

# Bitcoin Network Configuration
BITCOIN_NETWORK=regtest
BITCOIN_RPC_PORT=18443
BITCOIN_RPC_USERNAME=bitcoin
BITCOIN_RPC_PASSWORD=bitcoinpass

# Titan Configuration
TITAN_HTTP_PORT=3030
TITAN_TCP_PORT=8080
COMMIT_INTERVAL=5

# Docker Image Tag (examples: latest, v1.0.0, main, sha-abc123)
DOCKER_TAG=latest
